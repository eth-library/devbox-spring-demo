{
  "name": "Spring Boot Devbox Isolated Environment Demo",
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "forwardPorts": [8080],
  "remoteUser": "devbox",
  "postStartCommand": "bash -c 'source ~/.devbox/shell && mvn spring-boot:run'"
}